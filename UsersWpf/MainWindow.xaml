<mah:MetroWindow x:Class="UsersWpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UsersWpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="550" Width="890" MinHeight="550" MinWidth="850" >

    <Grid>

        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Menu Grid.Row="0">
            <MenuItem Header="File" >
                <MenuItem Header="Logout" Click="MenuItem_Click"></MenuItem>
            </MenuItem>
        </Menu>
        <Frame x:Name="NavigationFrame" Grid.Row="1" Source="./Login.xaml" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
    

    
    
</mah:MetroWindow>
